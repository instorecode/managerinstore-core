webapp.settings.env = PRODUCTION
production.hibernate.connection.driver_class = com.mysql.jdbc.Driver
production.hibernate.dialect = org.hibernate.dialect.MySQLDialect
production.hibernate.connection.url = jdbc:mysql://localhost:3306/managerinstore?autoReconnectForPools=true
production.hibernate.connection.username = root
production.hibernate.connection.password = 
production.javax.persistence.validation.mode = none
production.hibernate.connection.provider_class = org.hibernate.service.jdbc.connections.internal.C3P0ConnectionProvider
production.hibernate.connection.pool_size = 1
production.c3p0.min_size = 5
production.c3p0.max_size = 50
production.c3p0.max_statements = 0
production.c3p0.idle_test_period = 100
production.c3p0.timeout = (60 * 30)
production.c3p0.autoCommitOnClose = false
production.c3p0.acquire_increment = 5
